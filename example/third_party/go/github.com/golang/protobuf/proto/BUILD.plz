subinclude("//build_defs")

fileexport(
    name = "proto",
    srcs = [
        "proto/clone.go",
        "proto/decode.go",
        "proto/deprecated.go",
        "proto/discard.go",
        "proto/encode.go",
        "proto/equal.go",
        "proto/extensions.go",
        "proto/lib.go",
        "proto/message_set.go",
        "proto/pointer_reflect.go",
        "proto/pointer_unsafe.go",
        "proto/properties.go",
        "proto/table_marshal.go",
        "proto/table_merge.go",
        "proto/table_unmarshal.go",
        "proto/text.go",
        "proto/text_parser.go",
    ],
    tag = "go_source",
    deps = ["//example/third_party/go/github.com/golang/protobuf:_protobuf#download"],
